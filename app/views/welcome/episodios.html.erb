<h2 class="headertekst"><%=@serie %></h2>
<br>
<br>
<table style="width:50%" class="center">
  <tr>
    <th rowspan=@lista_episodios.length().to_Str><%= "Temporada "+@temporada %></th>
    <% @lista_episodios.each_pair do |k, v| %>
    <tr><td><%= link_to "Episodio "+k+": "+v, detalle_path(capitulo: k, serie: @serie) %></td></tr>
    <%end%>
  </tr>
  </table>
